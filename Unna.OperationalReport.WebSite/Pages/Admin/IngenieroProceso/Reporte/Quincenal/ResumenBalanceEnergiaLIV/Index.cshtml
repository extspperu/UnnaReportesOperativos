                                            <script>
                                                document.addEventListener('DOMContentLoaded', function () {
                                                    const onlyNumberInputs = document.querySelectorAll('.only-number');

                                                    onlyNumberInputs.forEach(input => {
                                                        input.addEventListener('input', function () {
                                                            // Remove invalid characters but keep hyphen at the beginning and decimal points
                                                            let value = this.value.replace(/[^0-9.-]/g, '');
                                                            const hyphenIndex = value.indexOf('-');

                                                            if (hyphenIndex > 0) {
                                                                // If hyphen is not at the start, remove it
                                                                value = value.replace(/-/g, '');
                                                                value = '-' + value;
                                                            }

                                                            this.value = value;
                                                        });

                                                        input.addEventListener('keydown', function (event) {
                                                            if (event.key === '-' && this.selectionStart !== 0) {
                                                                // Prevent entering hyphen anywhere other than the start
                                                                event.preventDefault();
                                                            }
                                                        });

                                                        input.addEventListener('blur', function () {
                                                            // Remove lone hyphen
                                                            if (this.value === '-') {
                                                                this.value = '';
                                                            }
                                                        });
                                                    });
                                                });
