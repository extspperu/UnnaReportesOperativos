@page
@model Unna.OperationalReport.WebSite.Pages.Admin.IngenieroProceso.Reporte.Quincenal.ComposicionGnaLIV_2.IndexModel
@{
}


@section Styles {
    <link href="~/css/reportes.css" rel="stylesheet" />
}

@section Scripts {
    <script src="~/js/admin/ingenieroproceso/reporte/diario/boletacnpc/index.js"></script>

}

<input type="hidden" id="__HD_URL_GENERAR_REPORTE" value="@Url.Content("~/api/admin/ingenieroProceso/reporte/diario/BoletaCnpc/GenerarExcel")" />

<div class="container">
    <div class="row">
        <div class="col-12">
            <table class="table tbl-resumen-ventas w-100">
                <tbody>
                    <tr>
                        <td rowspan="2"><img src="~/images/logo.png" alt="Unna Energía Logo" style="width: 11rem;"></td>
                        <td rowspan="2">
                            <h2>COMPOSICION QUINCENAL GNA LOTE IV</h2>
                        </td>
                        <td>UNNA ENERGÍA - ENEL</td>
                    </tr>
                    <tr>
                        <td>Versión 01</td>
                    </tr>
                </tbody>
            </table>


            <div class="container mt-3">
                <div class="container mt-3">

                    <div class="row">
                        <div class="w-100 mt-5 mb-1">
                            <h3 class="mb-5 mt-1 w-100"> COMPOSICIÓN DE GNA DE UNNA PROMEDIO MENSUAL (LOTE IV)</h3>
                            <table class="table table-bordered tbl-resumen-ventas" style="max-width:1300px;float:left;">
                                <thead>
                                    <tr>
                                        <th>DIA</th>
                                        <th>C6</th>
                                        <th>C3</th>
                                        <th>IC4</th>
                                        <th>NC4</th>
                                        <th>NEO C5</th>
                                        <th>IC5</th>
                                        <th>NC5</th>
                                        <th>NITROG</th>
                                        <th>C1</th>
                                        <th>CO2</th>
                                        <th>C2</th>
                                        <th>OBSERVACIONES</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.Dato.ComposicionGnaLIV_2DetComposicion)
                                    {
                                        <tr>
                                            <td width="100"><input type="text" class="form-control form-report only-number text-right" value="@item.CompGnaDia"></td>
                                            <td width="100"><input type="text" class="form-control form-report only-number text-right" value="@item.CompGnaC6"></td>
                                            <td width="100"><input type="text" class="form-control form-report only-number text-right" value="@item.CompGnaC3"></td>
                                            <td width="100"><input type="text" class="form-control form-report only-number text-right" value="@item.CompGnaIc4"></td>
                                            <td width="100"><input type="text" class="form-control form-report only-number text-right" value="@item.CompGnaNc4"></td>
                                            <td width="100"><input type="text" class="form-control form-report only-number text-right" value="@item.CompGnaNeoC5"></td>
                                            <td width="100"><input type="text" class="form-control form-report only-number text-right" value="@item.CompGnaIc5"></td>
                                            <td width="100"><input type="text" class="form-control form-report only-number text-right" value="@item.CompGnaNc5"></td>
                                            <td width="100"><input type="text" class="form-control form-report only-number text-right" value="@item.CompGnaNitrog"></td>
                                            <td width="100"><input type="text" class="form-control form-report only-number text-right" value="@item.CompGnaC1"></td>
                                            <td width="100"><input type="text" class="form-control form-report only-number text-right" value="@item.CompGnaCo2"></td>
                                            <td width="100"><input type="text" class="form-control form-report only-number text-right" value="@item.CompGnaC2"></td>
                                            <td width="100"><input type="text" class="form-control" value="@item.CompGnaObservacion"></td>
                                        </tr>
                                    }
                                    <tr>
                                        <td>Perupetro</td>
                                        <td><input type="text" class="form-control form-report only-number text-right" value="@Model.Dato.TotalPromedioPeruPetroC6"></td>
                                        <td><input type="text" class="form-control form-report only-number text-right" value="@Model.Dato.TotalPromedioPeruPetroC3"></td>
                                        <td><input type="text" class="form-control form-report only-number text-right" value="@Model.Dato.TotalPromedioPeruPetroIc4"></td>
                                        <td><input type="text" class="form-control form-report only-number text-right" value="@Model.Dato.TotalPromedioPeruPetroNc4"></td>
                                        <td><input type="text" class="form-control form-report only-number text-right" value="@Model.Dato.TotalPromedioPeruPetroNeoC5"></td>
                                        <td><input type="text" class="form-control form-report only-number text-right" value="@Model.Dato.TotalPromedioPeruPetroIc5"></td>
                                        <td><input type="text" class="form-control form-report only-number text-right" value="@Model.Dato.TotalPromedioPeruPetroNc5"></td>
                                        <td><input type="text" class="form-control form-report only-number text-right" value="@Model.Dato.TotalPromedioPeruPetroNitrog"></td>
                                        <td><input type="text" class="form-control form-report only-number text-right" value="@Model.Dato.TotalPromedioPeruPetroC1"></td>
                                        <td><input type="text" class="form-control form-report only-number text-right" value="@Model.Dato.TotalPromedioPeruPetroCo2"></td>
                                        <td><input type="text" class="form-control form-report only-number text-right" value="@Model.Dato.TotalPromedioPeruPetroC2"></td>
                                        <td></td>
                                    </tr>
                                    
                                </tbody>
                            </table>

                        </div>
                    </div>


                    <div class="row">
                        <div class="w-100 mt-5 mb-1">
                            <table class="table table-bordered tbl-resumen-ventas" style="max-width:500px;float:left;">
                                <thead>
                                    <tr>
                                        <th>ABREV.</th>
                                        <th>COMPONENTE</th>
                                        <th>MOL%</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.Dato.ComposicionGnaLIV_2DetComponente)
                                    {
                                        <tr>
                                            <td><input type="text" class="form-control form-report only-number text-right" value="@item.CompSimbolo"></td>
                                            <td><input type="text" class="form-control form-report only-number text-right" value="@item.CompDescripcion"></td>
                                            <td><input type="text" class="form-control form-report only-number text-right" value="@item.CompMolPorc"></td>
                                        </tr>
                                    }
                                </tbody>
                            </table>

                        </div>
                    </div>



                </div>
            </div>

            <!-- Cuadro Firma -->
            <div class="w-100 mt-20">
                <table style="max-width:100%;float:inherit;">
                    <thead>
                        <tr>
                            <th></th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td ><b><img src="~/images/firmas/FIRMA RSC.jpeg" style="max-width:220px;" /></b> </td>
                            <td ><b><img src="~/images/firmas/FIRMA RSC.jpeg" style="max-width:220px;" /></b> </td>
                        </tr>
                        <tr>
                            <td><b>----------------------------------------------------</b> </td>
                            <td><b>----------------------------------------------------</b> </td>
                        </tr>
                        <tr>
                            <td><b>PERUPETRO S.A</b> </td>
                            <td><b>UNNA ENERGIA S.A</b> </td>
                        </tr>
                    </tbody>
                </table>
            </div>

            <div class="w-100 mt-10 mb-10 text-right">
                <button id="btnDescargarExcel" class="btn btn-primary btn-lg">Descargar Excel</button>

            </div>

        </div>
    </div>
</div>

<script>