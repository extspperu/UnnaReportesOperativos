                                                    </div>
                                                    <div class="col-3 col-md-4">
                                                        <input type="text" class="form-control form-report only-number text-right" id="VolumenProduccionTotalGlpLoteIv" value="@Model.Dato?.VolumenProduccionTotalGlpLoteIv">
