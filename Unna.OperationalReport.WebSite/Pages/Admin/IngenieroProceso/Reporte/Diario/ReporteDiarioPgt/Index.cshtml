@page
@model Unna.OperationalReport.WebSite.Pages.Admin.IngenieroProceso.Reporte.Diario.ReporteDiarioPgt.IndexModel
@{
}

@section Styles {
    <link href="~/css/reportes.css" rel="stylesheet" />
}

@section Scripts {
    <script src="~/js/admin/ingenieroproceso/reporte/diario/ReporteDiarioPgt/index.js"></script>
}

<input type="hidden" id="__URL_OBTENER_REPORTE" value="@Url.Content("~/api/admin/ingenieroProceso/reporte/diario/reportediariopgt/Obtener")" />
<input type="hidden" id="__URL_GUARDAR_REPORTE" value="@Url.Content("~/api/admin/ingenieroProceso/reporte/diario/ReporteDiarioPgt/Guardar")" />
<input type="hidden" id="__URL_GENERAR_REPORTE_EXCEL" value="@Url.Content("~/api/admin/ingenieroProceso/reporte/diario/ReporteDiarioPgt/GenerarExcel")" />



<div class="container">
    <div class="row">
        <div class="col-12">
            <table class="table tbl-resumen-ventas w-100">
                <tbody>
                    <tr>
                        <td rowspan="3"><img src="@Url.Content("~/images/logo.png")" alt="Unna Energía Logo" style="width: 11rem;"></td>
                        <td rowspan="3">
                            <h2>@Model.Dato?.General?.NombreReporte</h2>
                        </td>
                        <td colspan="2">@Model.Dato?.General?.Nombre</td>
                    </tr>
                    <tr>
                        <td colspan="2">
                            <p class="mb-0">Versión/Fecha</p>
                            <p class="mb-0">@Model.Dato?.General?.Version / @Model.Dato?.General?.Fecha</p>
                        </td>
                    </tr>
                    <tr>
                        <td>Preparado por: @Model.Dato?.General?.PreparadoPör</td>
                        <td>Aprobado por: @Model.Dato?.General?.AprobadoPor</td>

                    </tr>
                </tbody>
            </table>


            <div class="container mt-3">
                <div class="mt-3">
                    <div class="row">
                        <div class="w-100 mt-5 mb-1">
                            <table class="table table-bordered tbl-resumen-ventas" style="max-width:300px;float: right;">
                                <tbody>
                                    <tr>
                                        <td><b>FECHA</b></td>
                                        <td><b>@Model?.Dato?.Fecha</b> </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12 mt-5 mb-1">
                            <h4 class="mb-5 mt-1 w-100">1. RECEPCION DE GAS NATURAL ASOCIADO (GNA):</h4>
                            <table class="table table-bordered tbl-resumen-ventas">
                                <thead>
                                    <tr>
                                        <th colspan="7">GAS NATURAL ASOCIADO (GNA)</th>
                                    </tr>
                                    <tr>
                                        <th rowspan="2">SUMINISTRADOR</th>
                                        <th>VOLUMEN DIARIO</th>
                                        <th>PODER CALORIFICO</th>
                                        <th colspan="2">RIQUEZA</th>
                                        <th>ENERGIA DIARIA</th>
                                        <th>VOLUMEN DEL PROMEDIO MENSUAL </th>
                                    </tr>
                                    <tr>
                                        <th>(MPCS)</th>
                                        <th>BTU/PC</th>
                                        <th>GAL/MPC</th>
                                        <th>BLS/MMPC</th>
                                        <th>MMBTU </th>
                                        <th>(MPCS)</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.Dato.GasNaturalAsociado)
                                    {
                                        <tr>
                                            <td>@item.Lote</td>
                                            <td><input type="text" class="form-control form-report only-number text-right" value="@item.Volumen"></td>
                                            <td><input type="text" class="form-control form-report only-number text-right" value="@item.Calorifico"></td>
                                            <td><input type="text" class="form-control form-report only-number text-right" value="@item.Riqueza"></td>
                                            <td><input type="text" class="form-control form-report only-number text-right" value="@item.RiquezaBls"></td>
                                            <td><input type="text" class="form-control form-report only-number text-right" value="@item.EnergiaDiaria"></td>
                                            <td><input type="text" class="form-control form-report only-number text-right" value="@item.VolumenPromedio"></td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>

                        <div class="col-md-6">
                            <div class="form-group row mb-0">
                                <label class="col-6 col-form-label">GAS PROCESADO:</label>
                                <div class="col-6">
                                    <input class="form-control form-report only-number text-right" type="text" value="@Model.Dato.GasProcesado" id="example-text-input" />
                                </div>
                            </div>
                            <div class="form-group row mb-0">
                                <label class="col-6 col-form-label">GAS NO PROCESADO:</label>
                                <div class="col-6">
                                    <input class="form-control form-report only-number text-right" type="text" value="@Model.Dato.GasNoProcesado" id="example-text-input" />
                                </div>
                            </div>
                            <div class="form-group row mb-0">
                                <label class="col-6 col-form-label">UTILIZACIÓN DE PLANTA PARIÑAS</label>
                                <div class="col-6">                                    
                                    <div class="input-group">
                                        <input type="text" class="form-control form-report only-number text-right" id="tbUtilizacionPlantaParinias" value="@Model.Dato.UtilizacionPlantaParinias">
                                        <div class="input-group-append"><span class="input-group-text">%</span></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group row">
                                <label class="col-6 col-form-label">HORAS DE LA PLANTA F/S</label>
                                <div class="col-6">
                                    <input type="text" class="form-control form-report only-number text-right" id="tbHoraPlantaFs" value="@Model.Dato.HoraPlantaFs">
                                    
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row mt-10">
                        <div class="col-md-9 mt-5 mb-1">
                            <h4 class="mb-5 mt-1 w-100">2. DISTRIBUCIÓN DE GAS NATURAL SECO TOTAL (GNS):</h4>
                            <table class="table table-bordered tbl-resumen-ventas">
                                <thead>
                                    <tr>
                                        <th colspan="5">GAS NATURAL SECO (GNS)</th>
                                    </tr>
                                    <tr>
                                        <th rowspan="2">DISTRIBUCION</th>
                                        <th>VOLUMEN DIARIO</th>
                                        <th>PODER CALORIFICO</th>
                                        <th>VOLUMEN DEL PROMEDIO MENSUAL </th>
                                        <th>ENERGIA DIARIA</th>
                                    </tr>
                                    <tr>
                                        <th>(MPCS)</th>
                                        <th>BTU/PC</th>
                                        <th>(MPCS)</th>
                                        <th>MMBTU </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model.Dato.GasNaturalSeco != null)
                                    {
                                        @foreach (var item in Model.Dato.GasNaturalSeco)
                                        {
                                            <tr>
                                                <td>@item.Distribucion</td>
                                                <td><input type="text" class="form-control form-report only-number text-right" value="@item.Volumen"></td>
                                                <td><input type="text" class="form-control form-report only-number text-right" value="@item.Calorifico"></td>
                                                <td><input type="text" class="form-control form-report only-number text-right" value="@item.VolumenPromedio"></td>
                                                <td><input type="text" class="form-control form-report only-number text-right" value="@item.EnergiaDiaria"></td>

                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <div class="row mt-10">
                        <div class="col-md-12">
                            <h4 class="mb-5 mt-1 w-100">3. PRODUCCIÓN Y VENTA DE LÍQUIDOS DE GAS NATURAL (LGN)</h4>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group row mb-0">
                                <label class="col-6 col-form-label">EFICIENCIA DE RECUPERACION DE LGN:</label>
                                <div class="col-3">
                                    <input class="form-control form-report only-number text-right" type="text" value="@Model.Dato.EficienciaRecuperacionLgn" id="example-text-input" />
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 mt-5">
                            <table class="table table-bordered tbl-resumen-ventas" style="max-width:1000px;float:left;">
                                <thead>
                                    <tr>
                                        <th rowspan="3">LIQUIDOS DE GAS NATURAL (LGN)</th>
                                        <th colspan="2">PRODUCCION</th>
                                        <th colspan="2">VENTAS</th>
                                    </tr>
                                    <tr>
                                        <th>DIARIA</th>
                                        <th>MENSUAL</th>
                                        <th>DIARIA</th>
                                        <th>MENSUAL</th>
                                    </tr>
                                    <tr>
                                        <th>BLS</th>
                                        <th>BLS</th>
                                        <th>BLS</th>
                                        <th>BLS</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model.Dato.LiquidosGasNaturalProduccionVentas != null)
                                    {
                                        @foreach (var item in Model.Dato.LiquidosGasNaturalProduccionVentas)
                                        {
                                            <tr>
                                                <td>@item.Producto</td>
                                                <td><input type="text" class="form-control form-report only-number text-right" value="@item.ProduccionDiaria"></td>
                                                <td><input type="text" class="form-control form-report only-number text-right" value="@item.ProduccionMensual"></td>
                                                <td><input type="text" class="form-control form-report only-number text-right" value="@item.VentaDiaria"></td>
                                                <td><input type="text" class="form-control form-report only-number text-right" value="@item.VentaMensual"></td>
                                            </tr>
                                        }
                                    }



                                </tbody>
                            </table>
                        </div>



                        <div class="col-md-12 mt-5">
                            <table class="table table-bordered tbl-resumen-ventas">
                                <thead>
                                    <tr>
                                        <th colspan="12">TANQUE DE DESPACHO, PLACAS DE CISTERNAS Y VOLÚMENES DESPACHADOS DE GLP (GAL)</th>
                                    </tr>
                                    <tr>
                                        <th>Datos</th>
                                        <th>1er despacho</th>
                                        <th>2do despacho</th>
                                        <th>3er despacho</th>
                                        <th>4to despacho</th>
                                        <th>5to despacho</th>
                                        <th>6to despacho</th>
                                        <th>7mo despacho</th>
                                        <th>8vo despacho</th>
                                        <th>9no despacho</th>
                                        <th>10mo despacho</th>
                                        <th>Total gal. GLP</th>
                                    </tr>
                                </thead>
                                @if (Model.Dato?.VolumenDespachoGlp != null)
                                {
                                    <tbody>
                                        <tr class="list-datos-tabla">
                                            <td>Tanque N°</td>
                                            @for (var i = 0; i < 10; i++)
                                            {
                                                if (Model.Dato.VolumenDespachoGlp.Count > i)
                                                {
                                                    <td>@Model.Dato.VolumenDespachoGlp[i].Tanque</td>
                                                }
                                                else
                                                {
                                                    <td></td>
                                                }
                                            }
                                            <td>@(Model.Dato.VolumenDespachoGlp.Count)</td>
                                        </tr>
                                        <tr class="list-datos-tabla">
                                            <td>Cliente</td>
                                            @for (var i = 0; i < 10; i++)
                                            {
                                                if (Model.Dato.VolumenDespachoGlp.Count > i)
                                                {
                                                    <td>@Model.Dato.VolumenDespachoGlp[i].Cliente</td>
                                                }
                                                else
                                                {
                                                    <td></td>
                                                }
                                            }
                                            <td></td>
                                        </tr>
                                        <tr class="list-datos-tabla">
                                            <td>Placa de Cisterna</td>
                                            @for (var i = 0; i < 10; i++)
                                            {
                                                if (Model.Dato.VolumenDespachoGlp.Count > i)
                                                {
                                                    <td>@Model.Dato.VolumenDespachoGlp[i].Placa</td>
                                                }
                                                else
                                                {
                                                    <td></td>
                                                }
                                            }
                                            <td></td>
                                        </tr>
                                        <tr class="list-datos-tabla">
                                            <td>Volumen</td>
                                            @for (var i = 0; i < 10; i++)
                                            {
                                                if (Model.Dato.VolumenDespachoGlp.Count > i)
                                                {
                                                    <td><input type="text" class="form-control form-report only-number text-right" value="@Model.Dato.VolumenDespachoGlp[i].Volumen"></td>
                                                }
                                                else
                                                {
                                                    <td></td>
                                                }
                                            }
                                            <td><input type="text" class="form-control form-report only-number text-right" value="@(Model.Dato.VolumenDespachoGlp.Sum(e => e.Volumen))"></td>
                                        </tr>

                                    </tbody>
                                }
                            </table>
                        </div>


                        <div class="col-md-12 mt-5">
                            <table class="table table-bordered tbl-resumen-ventas">
                                <thead>
                                    <tr>
                                        <th colspan="12">TANQUE DE DESPACHO, PLACAS DE CISTERNAS Y VOLÚMENES DESPACHADOS DE CGN (GAL)</th>
                                    </tr>
                                    <tr>
                                        <th>Datos</th>
                                        <th>1er despacho</th>
                                        <th>2do despacho</th>
                                        <th>3er despacho</th>
                                        <th>4to despacho</th>
                                        <th>5to despacho</th>
                                        <th>6to despacho</th>
                                        <th>7mo despacho</th>
                                        <th>8vo despacho</th>
                                        <th>9no despacho</th>
                                        <th>10mo despacho</th>
                                        <th>Total gal. GLP</th>
                                    </tr>
                                </thead>
                                @if (Model.Dato?.VolumenDespachoCgn != null)
                                {
                                    <tbody>
                                        <tr class="list-datos-tabla">
                                            <td>Tanque N°</td>
                                            @for (var i = 0; i < 10; i++)
                                            {
                                                if (Model.Dato.VolumenDespachoCgn.Count > i)
                                                {
                                                    <td>@Model.Dato.VolumenDespachoCgn[i].Tanque</td>
                                                }
                                                else
                                                {
                                                    <td></td>
                                                }
                                            }
                                            <td>@(Model.Dato.VolumenDespachoCgn.Count)</td>
                                        </tr>
                                        <tr class="list-datos-tabla">
                                            <td>Cliente</td>
                                            @for (var i = 0; i < 10; i++)
                                            {
                                                if (Model.Dato.VolumenDespachoCgn.Count > i)
                                                {
                                                    <td>@Model.Dato.VolumenDespachoCgn[i].Cliente</td>
                                                }
                                                else
                                                {
                                                    <td></td>
                                                }
                                            }
                                            <td></td>
                                        </tr>
                                        <tr class="list-datos-tabla">
                                            <td>Placa de Cisterna</td>
                                            @for (var i = 0; i < 10; i++)
                                            {
                                                if (Model.Dato.VolumenDespachoCgn.Count > i)
                                                {
                                                    <td>@Model.Dato.VolumenDespachoCgn[i].Placa</td>
                                                }
                                                else
                                                {
                                                    <td></td>
                                                }
                                            }
                                            <td></td>
                                        </tr>
                                        <tr class="list-datos-tabla">
                                            <td>Volumen</td>
                                            @for (var i = 0; i < 10; i++)
                                            {
                                                if (Model.Dato.VolumenDespachoCgn.Count > i)
                                                {
                                                    <td><input type="text" class="form-control form-report only-number text-right" value="@Model.Dato.VolumenDespachoCgn[i].Volumen"></td>
                                                }
                                                else
                                                {
                                                    <td></td>
                                                }
                                            }
                                            <td><input type="text" class="form-control form-report only-number text-right" value="@(Model.Dato.VolumenDespachoCgn.Sum(e => e.Volumen))"></td>
                                        </tr>

                                    </tbody>
                                }
                            </table>
                        </div>


                    </div>




                    <div class="col-md-12 mt-10">
                        <div class="row">
                            <div class="col-md-12">
                                <h4 class="mb-5 mt-1 w-100">4.  VOLUMEN DE GAS Y PRODUCCIÓN DE GNA ADICIONAL DEL LOTE X (CNPC PERÚ):</h4>
                            </div>
                            <div class="col-md-6">
                                <table class="table table-bordered tbl-resumen-ventas" style="max-width:500px;float:left;">
                                    <thead>
                                        <tr>
                                            <th colspan="2">GNA TOTAL DE CNPC</th>
                                        </tr>
                                        <tr>
                                            <th>SUMINISTRADOR</th>
                                            <th>VOLUMEN DIARIO (MPCS)</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model.Dato != null && Model.Dato.VolumenProduccionLoteXGnaTotalCnpc != null)
                                        {
                                            @foreach (var item in Model.Dato.VolumenProduccionLoteXGnaTotalCnpc)
                                            {
                                                <tr class="list-datos-tabla">
                                                    <td>@item.Nombre</td>
                                                    <td><input type="text" class="form-control form-report text-right" id="txtCapacidadInstalada_item.Item" value="@item.Volumen"></td>
                                                </tr>
                                            }
                                        }
                                    </tbody>

                                </table>
                            </div>
                            <div class="col-md-6">
                                <table class="table table-bordered tbl-resumen-ventas" style="max-width:500px;float:left;">
                                    <thead>
                                        <tr>
                                            <th colspan="2">LIQUIDOS DE GAS NATURAL (LGN)</th>
                                        </tr>
                                        <tr>
                                            <th>PRODUCTO</th>
                                            <th>VOLUMEN</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model.Dato != null && Model.Dato.VolumenProduccionLoteXLiquidoGasNatural != null)
                                        {
                                            @foreach (var item in Model.Dato.VolumenProduccionLoteXLiquidoGasNatural)
                                            {
                                                <tr class="list-datos-tabla">
                                                    <td>@item.Nombre</td>
                                                    <td><input type="text" class="form-control form-report text-right" id="txtCapacidadInstalada_item.Item" value="@item.Volumen"></td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>

                        </div>

                    </div>

                    <div class="col-md-12 mt-10">
                        <div class="row">
                            <div class="col-md-12">
                                <h5 class="mb-5 mt-1 w-100">5. VOLUMEN DE GAS Y PRODUCCIÓN DE ENEL:</h5>
                            </div>
                            <div class="col-md-6">
                                <table class="table table-bordered tbl-resumen-ventas" style="max-width:500px;float:left;">
                                    <thead>
                                        <tr>
                                            <th colspan="2">ENEL</th>
                                        </tr>
                                        <tr>
                                            <th>DISTRIBUCION (MPCSD)</th>
                                            <th>VOLUMEN (MPCS)</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model.Dato != null && Model.Dato.VolumenProduccionEnel != null)
                                        {
                                            @foreach (var item in Model.Dato.VolumenProduccionEnel)
                                            {
                                                <tr class="list-datos-tabla">
                                                    <td>@item.Nombre</td>
                                                    <td><input type="text" class="form-control form-report text-right" id="txtCapacidadInstalada_item.Item" value="@item.Volumen"></td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>

                            <div class="col-md-6">
                                <table class="table table-bordered tbl-resumen-ventas" style="max-width:500px;float:left;">
                                    <thead>
                                        <tr>
                                            <th colspan="2">LIQUIDOS DE GAS NATURAL (LGN)</th>
                                        </tr>
                                        <tr>
                                            <th>PRODUCTO</th>
                                            <th>VOLUMEN</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model.Dato != null && Model.Dato.VolumenProduccionGasNaturalEnel != null)
                                        {
                                            @foreach (var item in Model.Dato.VolumenProduccionGasNaturalEnel)
                                            {
                                                <tr class="list-datos-tabla">
                                                    <td>@item.Nombre</td>
                                                    <td><input type="text" class="form-control form-report text-right" id="txtCapacidadInstalada_item.Item" value="@item.Volumen"></td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>

                        </div>


                    </div>

                    <div class="col-md-12 mt-10">
                        <div class="row">
                            <div class="col-md-12">
                                <h4 class="mb-5 mt-1 w-100">6.  VOLUMEN DE GAS Y PRODUCCIÓN DE PETROPERU (LOTE I, VI y Z-69):</h4>
                            </div>
                            <div class="col-md-6">
                                <table class="table table-bordered tbl-resumen-ventas" style="max-width:600px;float:left;">
                                    <thead>
                                        <tr>
                                            <th colspan="3">PETROPERÚ</th>
                                        </tr>
                                        <tr>
                                            <th>SUMINISTRADOR</th>
                                            <th>GNA RECIBIDO (MPCS)</th>
                                            <th>GNS TRASFERIDO</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model.Dato != null && Model.Dato.VolumenProduccionPetroperu != null)
                                        {
                                            @foreach (var item in Model.Dato.VolumenProduccionPetroperu)
                                            {
                                                <tr class="list-datos-tabla">
                                                    <td>@item.Suministrador</td>
                                                    <td><input type="text" class="form-control form-report text-right" id="txtCapacidadInstalada_item.Item" value="@item.GnaRecibido"></td>
                                                    <td><input type="text" class="form-control form-report text-right" id="txtCapacidadInstalada_item.Item" value="@item.GnsTrasferido"></td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                            <div class="col-md-6">
                                <table class="table table-bordered tbl-resumen-ventas" style="max-width:600px;float:left;">
                                    <thead>
                                        <tr>
                                            <th colspan="4">VOLUMEN DE LIQUIDOS DE GAS NATURAL (LGN)</th>
                                        </tr>
                                        <tr>
                                            <th>PRODUCCION</th>
                                            <th>LOTE Z-69</th>
                                            <th>LOTE VI</th>
                                            <th>LOTE I</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model.Dato != null && Model.Dato.VolumenProduccionLiquidoGasNatural != null)
                                        {
                                            @foreach (var item in Model.Dato.VolumenProduccionLiquidoGasNatural)
                                            {
                                                <tr class="list-datos-tabla">
                                                    <td>@item.Produccion</td>
                                                    <td><input type="text" class="form-control form-report text-right" id="txtCapacidadInstalada_item.Item" value="@item.LoteZ69"></td>
                                                    <td><input type="text" class="form-control form-report text-right" id="txtCapacidadInstalada_item.Item" value="@item.LoteVi"></td>
                                                    <td><input type="text" class="form-control form-report text-right" id="txtCapacidadInstalada_item.Item" value="@item.LoteI"></td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
