@page
@model Unna.OperationalReport.WebSite.Pages.Admin.IngenieroProceso.Reporte.Diario.BoletaBalanceEnergia.IndexModel
@{
}


@section Styles {
    <link href="~/css/reportes.css" rel="stylesheet" />
}

@section Scripts {
    <script src="~/js/admin/ingenieroproceso/reporte/diario/BoletaBalanceEnergia/index.js"></script>
}

<input type="hidden" id="__URL_GENERAR_REPORTE_EXCEL" value="@Url.Content("~/api/admin/ingenieroProceso/reporte/diario/BoletaBalanceEnergia/GenerarExcel")" />

<div class="container">
    <div class="row">
        <div class="col-12">
            <table class="table tbl-resumen-ventas w-100">
                <tbody>
                    <tr>
                        <td rowspan="2"><img src="~/images/logo.png" alt="Unna Energía Logo" style="width: 11rem;"></td>
                        <td rowspan="2">
                            <h2>BALANCE DE ENERGIA DIARIO DE GNA, GNS Y COMPONENTES PESADOS</h2>
                        </td>
                        <td>UNNA ENERGÍA - ENEL</td>
                    </tr>
                    <tr>
                        <td>Versión 01</td>
                    </tr>
                </tbody>
            </table>


            <div class="container mt-3">
                <div class="container mt-3">
                    <div class="row">
                        <div class="w-100 mt-5 mb-1">
                            <table class="table table-bordered tbl-resumen-ventas" style="max-width:300px;float: right;">
                                <tbody>
                                    <tr>
                                        <td><b>FECHA</b></td>
                                        <td><b>@Model.Dato?.Fecha</b> </td>
                                    </tr>

                                </tbody>
                            </table>
                        </div>
                        @* <h3 class="mb-5 mt-1 w-100">Cuadro N°1. Resumen de Ventas</h3> *@
                    </div>

                    <div class="row">
                        <div class="w-100 mt-5 mb-1">
                            <table class="table table-bordered tbl-resumen-ventas" style="max-width:500px;float:left;">
                                <thead>
                                    <tr>
                                        <th>

                                        </th>
                                        <th>
                                            Entregas
                                        </th>
                                        <th>Volumen MPCSD</th>
                                        <th>Poder Calorifico Bruto Btu/PC</th>
                                        <th>Energía MMBTU</th>
                                        <th>Riqueza Gal/MPC</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><label class="font-weight-bold">GNA Entregado a UNNA ENERGIA Gas Natural</label></td>
                                        <td><label class="font-weight-bold">CNPC PERU</label></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.GnaEntregaUnna?.Volumen"></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.GnaEntregaUnna?.PoderCalorifico"></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.GnaEntregaUnna?.Energia"></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.GnaEntregaUnna?.Riqueza"></td>
                                    </tr>
                                </tbody>
                            </table>

                            <h6>LIQUIDOS (Barriles)</h6>
                            <table class="table table-bordered tbl-resumen-ventas" style="max-width:460px;float:right;">
                                <thead>
                                    <tr>
                                        <th></th>
                                        <th>ENEL</th>
                                        <th>BLSD TOTAL</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.Dato.LiquidosBarriles)
                                    {
                                        <tr>
                                            <td><input type="text" class="form-control" value="@item.Nombre"></td>
                                            <td><input type="text" class="form-control" value="@item.Enel"></td>
                                            <td><input type="text" class="form-control" value="@item.Blsd"></td>
                                        </tr>
                                    }
                                </tbody>
                            </table>

                        </div>
                        @* <h3 class="mb-5 mt-1 w-100">Cuadro N°1. Resumen de Ventas</h3> *@
                    </div>

                    <div class="row">
                        <div class="w-100 mt-5 mb-1">
                            <h5 class="mb-5 mt-1 w-100" style="float:right;">EFICIENCIA DE RECUPERACION DE LGN</h5>
                            <table class="table table-bordered tbl-resumen-ventas" style="max-width:460px;float:right;">
                                <tbody>
                                    <tr>
                                        <td><label class="font-weight-bold">Com. Pesados del GNA (BSD)</label></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.ComPesadosGna"></td>
                                    </tr>
                                    <tr>
                                        <td><label class="font-weight-bold">% Eficiencia</label></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.PorcentajeEficiencia"></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <div class="row">
                        <div class="w-100 mt-5 mb-1">
                            <table class="table table-bordered tbl-resumen-ventas" style="max-width:460px;float:right;">
                                <tbody>
                                    <tr>
                                        <td><label class="font-weight-bold">Contenido Calórico promedio del  LGN</label></td>
                                        <td><b><input type="text" class="form-control" value="@Model.Dato?.ContenidoCalorificoPromLgn"></b></td>

                                        <td><label class="font-weight-bold">MMBTU/Bbl</label></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <label class="font-weight-bold">El contenido calórico del LGN corresponde a un valor histórico. Se deberá efectuar un </label>
                    </div>
                    

                    <div class="row">
                        <div class="w-100 mt-5 mb-1">
                            <table class="table table-bordered tbl-resumen-ventas" style="max-width:500px;float:left;">
                                <thead>
                                    <tr>
                                        <th>
                                        </th>
                                        <th>
                                            Distribución
                                        </th>
                                        <th>Volumen MPCSD</th>
                                        <th>Poder Calorifico Bruto Btu/PC</th>
                                        <th>Energía MMBTU</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.Dato.GnsAEnel)
                                    {
                                        <tr>
                                            <td>@item.Nombre</td>
                                            <td><input type="text" class="form-control" value="@item.Distribucion"></td>
                                            <td><input type="text" class="form-control" value="@item.Volumen"></td>
                                            <td><input type="text" class="form-control" value="@item.PoderCalorifico"></td>
                                            <td><input type="text" class="form-control" value="@item.PoderCalorifico"></td>
                                        </tr>
                                    }
                                    @foreach (var item2 in Model.Dato.ConsumoPropio)
                                    {
                                        <tr>
                                            <td>@item2.Nombre</td>
                                            <td><input type="text" class="form-control" value="@item2.Distribucion"></td>
                                            <td><input type="text" class="form-control" value="@item2.Volumen"></td>
                                            <td><input type="text" class="form-control" value="@item2.PoderCalorifico"></td>
                                            <td><input type="text" class="form-control" value="@item2.PoderCalorifico"></td>
                                        </tr>
                                    }
                                    @foreach (var item3 in Model.Dato.GnsAEnel)
                                    {
                                        <tr>
                                            <td>@item3.Nombre</td>
                                            <td><input type="text" class="form-control" value="@item3.Distribucion"></td>
                                            <td><input type="text" class="form-control" value="@item3.Volumen"></td>
                                            <td><input type="text" class="form-control" value="@item3.PoderCalorifico"></td>
                                            <td><input type="text" class="form-control" value="@item3.PoderCalorifico"></td>
                                        </tr>
                                    }
                                </tbody>
                            </table>

                            <table class="table table-bordered tbl-resumen-ventas" style="max-width:460px;float:right;">
                                <thead>
                                    <tr>
                                        <th>BALANCE</th>
                                        <th>MPCSD</th>
                                        <th>Barriles LGN</th>
                                        <th>Energia MMBTU</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><label class="font-weight-bold">Entregas de GNA</label></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.EntregaGna?.Mpcsd"></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.EntregaGna?.Barriles"></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.EntregaGna?.Energia"></td>
                                    </tr>
                                    <tr>
                                        <td><label class="font-weight-bold">GNS Restituido a ENEL</label></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.GnsRestituido?.Mpcsd"></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.GnsRestituido?.Barriles"></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.GnsRestituido?.Energia"></td>
                                    </tr>
                                    <tr>
                                        <td><label class="font-weight-bold">GNS Consumo Propio UNNA </label></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.GnsConsumoPropio?.Mpcsd"></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.GnsConsumoPropio?.Barriles"></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.GnsConsumoPropio?.Energia"></td>
                                    </tr>
                                    <tr>
                                        <td><label class="font-weight-bold">Recuperación de  Com. Pes.</label></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.Recuperacion?.Mpcsd"></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.Recuperacion?.Barriles"></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.Recuperacion?.Energia"></td>
                                    </tr>
                                    <tr>
                                        <td colspan="3" ><label class="font-weight-bold">DIFERENCIA ENERGETICA</label></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.DiferenciaEnergetica"></td>
                                    </tr>
                                    <tr>
                                        <td colspan="3"><label class="font-weight-bold">EXCESO EN CONSUMO PROPIO</label></td>
                                        <td><input type="text" class="form-control form-report text-center" value="@Model.Dato?.ExesoConsumoPropio"></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <div class="row">
                        <div class="w-100 mt-5 mb-1">
                            <h5>Comentarios:</h5>
                            <textarea class="form-control form-report text-center">@Model.Dato?.Comentario</textarea>
                          
                        </div>
                    </div>
                </div>
            </div>


            <!-- Cuadro Firma -->
            <div class="w-100 mt-20">
                <table style="max-width:100%;float:center;">
                    <thead>
                        <tr>
                            <th></th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td><b><img src="~/images/firmas/FIRMA RSC.jpeg" style="max-width:220px;" /></b> </td>
                            <td><b><img src="~/images/firmas/FIRMA RSC.jpeg" style="max-width:220px;" /></b> </td>
                        </tr>
                        <tr>
                            <td><b>----------------------------------------------------</b> </td>
                            <td><b>----------------------------------------------------</b> </td>
                        </tr>
                        <tr>
                            <td><b>Representante UNNA ENERGIA S.A</b> </td>
                            <td><b>Representante ENEL Generacion Piura S.A.</b> </td>
                        </tr>
                    </tbody>
                </table>
            </div>

            <div class="w-100 mt-10 mb-10 text-right">
                <button id="btnDescargarExcel" class="btn btn-primary btn-lg">Descargar Excel</button>

            </div>

        </div>
    </div>
</div>

<script>